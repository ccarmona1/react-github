{
  "name": "react-github",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "React GitHub application with TypeScript and Vite",
  "author": "crisman_carmona@epam.com",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
