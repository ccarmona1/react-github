    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
